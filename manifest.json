{
  "manifest_version": 2,

  "name": "Reminders",
  "description": "Gives you a gentle reminder when you need it.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "notifications",
    "https://ajax.googleapis.com/"
  ]
}
